<?xml version="1.0" encoding="utf-8" ?>
<ArrayOfWebpartPageDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <WebpartPageDefinition>
    <PageUrl>VideoGallery.aspx</PageUrl>
    <FileName>VideoGallery.aspx</FileName>
    <Title>Home</Title>
    <!--<Overwrite>true</Overwrite>-->
    <Webparts xsi:type="DefaultWP">
      <Title>Video Gallery</Title>
      <ZoneId>Body</ZoneId>
      <Index>1</Index>
      <Hidden>false</Hidden>
      <AllowDuplicate>false</AllowDuplicate>
      <WebpartName>VideoPlayerWebPart.webpart</WebpartName>
      <Properties>
        <Add Name="PlayCurrentContext" Value="True" Type="string" ></Add>
        <Add Name="AllowMultiple" Value="True" Type="string" ></Add>
        <Add Name="VideoExtension" Value="flv;mp4" Type="string" ></Add>
      </Properties>
    </Webparts>
    
    <Webparts xsi:type="DefaultWP">
      <Title>Comment box</Title>
      <ZoneId>RightColumn</ZoneId>
      <Index>1</Index>
      <Hidden>false</Hidden>
      <AllowDuplicate>false</AllowDuplicate>
      <WebpartName>CommentBoxWebPart.webpart</WebpartName>
      <Properties>
        <Add Name="Title" Value="$Resources:GalleryResources,CommentBoxTitle;" type="string"></Add>
      </Properties>
    </Webparts>
    
  
</WebpartPageDefinition>
</ArrayOfWebpartPageDefinition>