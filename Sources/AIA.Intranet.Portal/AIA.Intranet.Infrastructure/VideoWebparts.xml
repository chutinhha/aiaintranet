<?xml version="1.0" encoding="utf-8" ?>
<ArrayOfWebpartPageDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <WebpartPageDefinition>
    <PageUrl>/Pages/default.aspx</PageUrl>
    <Webparts xsi:type="DefaultWP">
      <Title>Video mới nhất</Title>
      <ZoneId>MyTop</ZoneId>
      <Index>1</Index>
      <Hidden>false</Hidden>
      <AllowDuplicate>false</AllowDuplicate>
      <WebpartName>VideoPlayerWebPart.webpart</WebpartName>
      <Properties>
        <Add Name="PlayLastestVideo" Value="true" Type="string" ></Add>
        <Add Name="AllowMultiple" Value="True" Type="string" ></Add>
        <Add Name="VideoExtension" Value="flv;mp4" Type="string" ></Add>
      </Properties>
    </Webparts>

    <Webparts xsi:type="XSLTListViewWP">
      <Title>Video Albums</Title>
      <ZoneId>RightColumn</ZoneId>
      <Index>1</Index>
      <Hidden>false</Hidden>
      <AllowDuplicate>false</AllowDuplicate>
      <ListName>Album</ListName>
      <ListUrl>/Lists/Album</ListUrl>
      <ViewName>Albums</ViewName>
      <CreateDefaultWP>true</CreateDefaultWP>
      <Properties>
        <Add Name="Title" Value="Video Albums" Type="string" />
      </Properties>
    </Webparts>

  </WebpartPageDefinition>
</ArrayOfWebpartPageDefinition>